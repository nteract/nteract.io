{
  "name": "nteract.io",
  "description": "The server for nteract.io",
  "version": "2.0.0",
  "author": "nteract contributors",
  "bugs": {
    "url": "https://github.com/nteract/nteract.io/issues"
  },
  "dependencies": {
    "express": "4.18.2",
    "isomorphic-fetch": "3.0.0",
    "mdi-react": "8.4.0",
    "next": "15.5.4",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-fns": "1.4.0",
    "react-syntax-highlighter": "9.0.1",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-styled-components": "2.0.7",
    "lint-staged": "12.5.0",
    "prettier": "2.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "homepage": "https://github.com/nteract/nteract.io#readme",
  "license": "BSD-3-Clause",
  "lint-staged": {
    "./**/*.{js,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nteract/nteract.io.git"
  },
  "scripts": {
    "now-build": "npm run build",
    "prebuild": "tsx fetch-members.ts",
    "build": "next build",
    "dev": "npm run prebuild && tsx server.ts",
    "prettier": "prettier --write './**/*.{js,ts,tsx}'",
    "start": "NODE_ENV=production tsx server.ts",
    "type-check": "tsc --noEmit"
  }
}
